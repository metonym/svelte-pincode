{
  "name": "svelte-pincode",
  "version": "2.2.1",
  "license": "MIT",
  "description": "Declarative pin code component for Svelte",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./src/index.js",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "svelte": "./src/index.js"
    },
    "./*.svelte": {
      "types": "./src/*.svelte.d.ts",
      "import": "./src/*.svelte"
    },
    "./unstyled/*.svelte": {
      "types": "./src/unstyled/*.svelte.d.ts",
      "import": "./src/unstyled/*.svelte"
    }
  },
  "scripts": {
    "dev": "bun --bun rollup -cw",
    "build": "bun --bun rollup -c",
    "test": "bun --bun svelte-check --workspace test"
  },
  "devDependencies": {
    "svelte": "^3.59.1",
    "svelte-check": "^3.7.1",
    "svelte-readme": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metonym/svelte-pincode.git"
  },
  "homepage": "https://github.com/metonym/svelte-pincode",
  "bugs": "https://github.com/metonym/svelte-pincode/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "pincode",
    "lockbox",
    "input"
  ],
  "files": [
    "src"
  ],
  "type": "module"
}
